#!name=All-in-One Unlock 
#!desc=Ported Scripts from AppTesters 
#!category=APP 
#!author=https://github.com/KawOat9  
#!icon=https://raw.githubusercontent.com/KawOat9/icons/main/AppTesters.png  
#!system=ios  

[Header Rewrite] 
# RevenueCat Force Refresh (Delete ETag) 
http-request ^https?://api.revenuecat.com/.+/(receipts$|subscribers/?(.*?)*$) header-del X-RevenueCat-ETag  
http-request ^https?://api.revenuecat.com/.+/(receipts$|subscribers/?(.*?)*$) header-del x-revenuecat-etag  

[MITM]  
hostname = %APPEND% api.revenuecat.com, buy.itunes.apple.com, api.picsart.com, api-account.kinemasters.com, us-central1-alight-creative.cloudfunctions.net, api.adapty.io, carrotweather.herokuapp.com, order.creativeappnow.com, api.mwm-users.com, us-central1-muslim-pro-app.cloudfunctions.net, prod.studysmarter.de, api.qonversion.io, subscription.grammarly.com, api.esound.app, api.purchasely.io, api.sandbox.love, subscriptions.songshift.com, api.adblockpro.app, mobile-api.adguard.org, license.pdfexpert.com, api-fra.vivacut.com   

[Script]   
# RevenueCat Universal 
AppRCTesters = type=http-response,pattern=^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/[^/]+$),requires-body=1,script-path=https://apptesters.org/egern/AutoRC.js 

RC_ETag_Cleaner = type=http-request,pattern=^https:\/\/api\.(revenuecat|rc-backup)\.com\/v1\/(subscribers|receipts),script-path=https://raw.githubusercontent.com/KawOat9/Scripts/main/deleteHeaderNotif.js   

# Adapty   
AppAdTesters = type=http-response,pattern=https://api.adapty.io/api/v1/sdk/analytics/profiles,requires-body=1,script-path=https://apptesters.org/egern/Adapty.js    

# Facemoji     
AppFMTesters = type=http-response,pattern=https://order.creativeappnow.com/iap/receipt,requires-body=1,script-path=https://apptesters.org/egern/Facemoji.js     

# Readdle (PDF Expert etc) 
AppReTesters = type=http-response,pattern=https:\/\/license\.pdfexpert\.com\/api\/2\.0\/(calendarslite|pdfexpert6|documents)\/subscription\/(refresh|check),requires-body=1,script-path=https://apptesters.org/egern/readdle.js   

# iTunes Receipt   
AppitTesters = type=http-response,pattern=^https:\/\/buy\.itunes\.apple\.com\/verifyReceipt,requires-body=1,script-path=https://apptesters.org/egern/iTunes.js   

# Grammarly 
AppGrTesters = type=http-response,pattern=^https?:\/\/subscription\.grammarly\.com\/api\/v1\/subscription$,requires-body=1,script-path=https://apptesters.org/egern/Grammarly.js    

# eSound    
AppeSTesters = type=http-response,pattern=api.esound.app/account,requires-body=1,script-path=https://apptesters.org/egern/eSound.js    

# Purchasely      
AppPurTesters = type=http-response,pattern=https://api.purchasely.io/paab/user_purchases,requires-body=1,script-path=https://apptesters.org/egern/purchasely.js      

# Vaia / StudySmarter 
AppVaTesters = type=http-response,pattern=prod.studysmarter.de/payments/info,requires-body=1,script-path=https://apptesters.org/egern/Vaia.js     

# Qonversion     
AppQOTesters = type=http-response,pattern=api.qonversion.io/v1/user/init,requires-body=1,script-path=https://apptesters.org/egern/qonversion.js     

# edjing Mix        
AppDJTesters = type=http-response,pattern=https://api.mwm-users.com/me/features,requires-body=1,script-path=https://apptesters.org/egern/edjingMix.js        

# VivaCut 
AppVCTesters = type=http-response,pattern=https://api-fra.vivacut.com/api/rest/commerce/integrate/vip/perform,requires-body=1,script-path=https://apptesters.org/egern/VivaCut.js   

# Carrot Weather     
AppCaTesters = type=http-response,pattern=https://carrotweather.herokuapp.com/parse/functions/findSubscriptionsForUserId,requires-body=1,script-path=https://apptesters.org/egern/Carrot.js     

# KineMaster    
AppKineTesters = type=http-response,pattern=https://api-account.kinemasters.com/v2/user/subscribe,requires-body=1,script-path=https://apptesters.org/egern/KineMaster.js        

# PicsArt (Note: This is http-request) 
AppPicTesters = type=http-request,pattern=https://api.picsart.com/gw-v2/shop/subscription/apple/purchases,requires-body=1,script-path=https://apptesters.org/egern/PicsArt.js       

# Alight Motion  
AppAMTesters = type=http-response,pattern=https://us-central1-alight-creative.cloudfunctions.net/getAccountStatusAndLicenses,requires-body=1,script-path=https://apptesters.org/egern/AlightMotion.js       

# Sandbox         
AppSaTesters = type=http-response,pattern=api.sandbox.love/accounts/current,requires-body=1,script-path=https://apptesters.org/egern/SandBox.js         

# SongShift 
AppSSesters = type=http-response,pattern=subscriptions.songshift.com/validate,requires-body=1,script-path=https://apptesters.org/egern/SongShift.js 

# AdblockPro 
AppADBTesters = type=http-response,pattern=api.adblockpro.app/verify,requires-body=1,script-path=https://apptesters.org/egern/AdblockPro.js 

# AdGuard Pro 
AppADGTesters = type=http-response,pattern=^https:\/\/mobile-api\.adguard\.org\/api\/.+\/ios_validate_receipt\/(.*?),requires-body=1,script-path=https://apptesters.org/egern/AdGuardPro.js 
